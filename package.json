{
  "name": "ProjectTagus",
  "version": "0.0.1",
  "description": "A Simple CMS using only Javascript.",
  "main": "app.js",
  "scripts": {
    "_test": "mocha ./tagus/tagus-cms/server/tests/*.js",
    "_buildDB": "node ./tagus/tagus-cms/server/buildDB/buildScript",
    "_dropDB": "node ./tagus/tagus-cms/server/buildDB/dropScript",
    "_rebuildDB": "npm run _dropDB && npm run _buildDB",
    "_buildClient": "cd tagus/tagus-client && npm run build && rm -rf ../tagus-cms/client/build && cp -a build ../tagus-cms/client/build/",
    "start": "concurrently \"nodemon app.js\" \"cd tagus/tagus-client && npm start\""
  },
  "author": "Snot",
  "license": "ISC",
  "dependencies": {
    "async": "^2.6.4",
    "axios": "^0.19.0",
    "bcrypt-nodejs": "0.0.3",
    "bluebird": "^3.4.7",
    "body-parser": "^1.15.0",
    "cookie-parser": "^1.4.3",
    "dot-env": "0.0.1",
    "dotenv": "^8.0.0",
    "ejs": "^2.5.7",
    "express": "^4.13.4",
    "hbs": "^4.0.0",
    "jwt-simple": "^0.5.1",
    "mongoose": "^4.4.19",
    "morgan": "^1.7.0",
    "multer": "^1.3.0",
    "nodemailer": "^4.6.3",
    "open": "0.0.5",
    "passport": "^0.3.2",
    "passport-jwt": "^3.0.0",
    "passport-local": "^1.0.0",
    "passport-token": "^0.1.3",
    "path": "^0.12.7",
    "rfr": "^1.2.3",
    "schema-object": "^4.0.11",
    "string": "^3.3.3",
    "tagus-cms": "0.0.6",
    "universal-cookie": "^2.1.2"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-http": "^2.0.1",
    "concurrently": "^3.5.0",
    "mocha": "^2.5.3",
    "nodemon": "^1.12.1"
  }
}
